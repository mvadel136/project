أدنى = 1;         
أعلى = 100;    
الهدف = 47;        

مادام (أدنى <= أعلى) {
    وسط = (أدنى + أعلى) / 2; 
    طباعة(وسط); 

    إذا (وسط == الهدف) {
        طباعة("تم العثور على الرقم!");
        توقف; 
    } وإلا إذا (وسط < الهدف) {
        أدنى = وسط + 1; 
    } وإلا {
        أعلى = وسط - 1; 
    }
}
